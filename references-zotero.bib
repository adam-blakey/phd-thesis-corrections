@article{clarkComputationalModelingInteractions2021a,
  title = {Computational Modeling of the Interactions between the Maternal and Fetal Circulations in Human Pregnancy},
  author = {Clark, Alys R. and Lee, Tet Chuan and James, Joanna L.},
  date = {2021-01},
