@article{hirschmuglPlacentalMobilizationFree2021,
  title = {Placental Mobilization of Free Fatty Acids Contributes to Altered Materno-Fetal Transfer in Obesity},
  author = {Hirschmugl, Birgit and Perazzolo, Simone and Sengers, Bram G. and Lewis, Rohan M. and Gruber, Michael and Desoye, Gernot and Wadsack, Christian},
  date = {2021-05},
  journaltitle = {International Journal of Obesity},
  shortjournal = {Int J Obes},
  volume = {45},
