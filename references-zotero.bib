  title = {Modeling and {{Analysis}} of {{Carbonate Matrix Acidizing Using}} a {{New Two-Scale Continuum Model}}},
  author = {Jia, Cunqi and Sepehrnoori, Kamy and Huang, Zhaoqin and Yao, Jun},
  date = {2021-10-13},
