@article{longFluidStructureInteraction2012,
  title = {Fluid–Structure Interaction Simulations of the {{Fontan}} Procedure Using Variable Wall Properties},
  author = {Long, C. C. and Hsu, M‐C. and Bazilevs, Y. and Feinstein, J. A. and Marsden, A. L.},
  date = {2012-05},
  journaltitle = {International Journal for Numerical Methods in Biomedical Engineering},
  shortjournal = {Numer Methods Biomed Eng},
