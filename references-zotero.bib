@article{flouriImprovedFetalBlood2020,
  title = {Improved Fetal Blood Oxygenation and Placental Estimated Measurements of Diffusion‐weighted {{MRI}} Using Data‐driven {{Bayesian}} Modeling},
  author = {Flouri, Dimitra and Owen, David and Aughwane, Rosalind and Mufti, Nada and Maksym, Kasia and Sokolska, Magdalena and Kendall, Giles and Bainbridge, Alan and Atkinson, David and Vercauteren, Tom and Ourselin, Sebastien and David, Anna L. and Melbourne, Andrew},
  date = {2020-06},
