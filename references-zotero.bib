  title = {Human Placental Villi Contain Stromal Macrovesicles Associated with Networks of Stellate Cells},
  author = {Palaiologou, E. and Etter, O. and Goggin, P. and Chatelet, D. S. and Johnston, D. A. and Lofthouse, E. M. and Doherty, R. and Pearson‐Farr, J. and Sengers, B. G. and Torrens, C. and Cleal, J. K. and Page, A. M. and Lewis, R. M.},
  date = {2020-01},
  journaltitle = {Journal of Anatomy},
