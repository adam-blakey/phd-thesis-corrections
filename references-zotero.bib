@article{collisEffectiveEquationsGoverning2017,
  title = {Effective Equations Governing an Active Poroelastic Medium},
  author = {Collis, J. and Brown, D. L. and Hubbard, M. E. and O’Dea, R. D.},
  date = {2017-02},
  journaltitle = {Proceedings of the Royal Society A: Mathematical, Physical and Engineering Sciences},
