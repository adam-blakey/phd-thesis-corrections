@article{declercqHighresolutionContrastenhancedMicroCT2019,
  title = {High-Resolution Contrast-Enhanced {{microCT}} Reveals the True Three-Dimensional Morphology of the Murine Placenta},
  author = {De Clercq, Katrien and Persoons, Eleonora and Napso, Tina and Luyten, Catherine and Parac-Vogt, Tatjana N. and Sferruzzi-Perri, Amanda N. and Kerckhofs, Greet and Vriens, Joris},
