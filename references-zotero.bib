@article{odeaMultiscaleAnalysisNutrient2015,
  title = {A Multiscale Analysis of Nutrient Transport and Biological Tissue Growth in Vitro},
  author = {O'Dea, Reuben D. and Nelson, M. R. and Haj, A.J. El and Waters, Sarah L. and Byrne, Helen M.},
  date = {2015-09-01},
